{
  "name": "gifken",
  "version": "2.0.0",
  "description": "TypeScript / JavaScript GIF parser and maker",
  "main": "lib/gifken.cjs.js",
  "module": "lib/gifken.esm.js",
  "browser": "lib/gifken.umd.js",
  "types": "lib/index.d.ts",
  "homepage": "https://aaharu.github.io/gifken/",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "pretest": "rimraf coverage",
    "clean": "rimraf lib coverage docs",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts --ignore-path .gitignore .",
    "doc": "typedoc --target es2015 --module commonjs --out docs --readme README.md src && node d.js",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aaharu/gifken.git"
  },
  "author": "aaharu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aaharu/gifken/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "browser-or-node": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.30",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.1.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.2.1",
    "typedoc": "^0.17.3",
    "typescript": "^3.8.3"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "animated-gif",
    "gif"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix"
    ]
  }
}
